<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:clickable="true"
    android:id="@+id/background"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline60"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.0" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline61"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.5" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline62"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.68" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline63"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="1.0"
        />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline64"
        app:layout_constraintGuide_percent="0"
        android:orientation="horizontal" />

    <ImageView
        android:id="@+id/land"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:src="@drawable/land"
        app:layout_constraintRight_toLeftOf="@+id/guideline61"
        android:layout_marginRight="8dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/guideline60"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="0dp"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintTop_toTopOf="@+id/guideline62"
        android:layout_marginTop="8dp"
        app:layout_constraintVertical_bias="0.0" >

        </ImageView>
    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline651"
        app:layout_constraintGuide_percent="0.18"

        android:orientation="vertical" />
    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline652"
        app:layout_constraintGuide_percent="0.38"

        android:orientation="vertical" />

    <android.support.constraint.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/signboard"
        android:layout_marginBottom="0dp"
        app:layout_constraintBottom_toTopOf="@+id/guideline70"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="@id/guideline651"
        app:layout_constraintRight_toLeftOf="@+id/guideline652"
        android:layout_marginRight="0dp"
        app:layout_constraintHorizontal_bias="0.568"
        app:layout_constraintTop_toTopOf="@+id/guideline68"
        android:layout_marginTop="0dp"
        app:layout_constraintVertical_bias="0.0">
        <android.support.constraint.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideline656"
            app:layout_constraintGuide_percent="0.6"

            android:orientation="horizontal" />
        <android.support.constraint.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideline659"
            app:layout_constraintGuide_percent="0.22"

            android:orientation="horizontal" />
        <android.support.constraint.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideline658"
            app:layout_constraintGuide_percent="0.4"

            android:orientation="horizontal" />
        <android.support.constraint.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideline670"
            app:layout_constraintGuide_percent="0"

            android:orientation="vertical" />
        <android.support.constraint.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideline671"
            app:layout_constraintGuide_percent="1"

            android:orientation="vertical" />
        <TextView
            android:id="@+id/username"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:text="TextView"
            android:gravity="center"
            tools:layout_editor_absoluteX="133dp"
            android:layout_marginTop="8dp"
            android:layout_marginRight="0dp"
            app:layout_constraintRight_toRightOf="@id/guideline671"
            android:layout_marginLeft="0dp"
            app:layout_constraintLeft_toLeftOf="@+id/guideline670"
            app:layout_constraintTop_toTopOf="@+id/guideline658"
            android:layout_marginBottom="0dp"
            app:layout_constraintBottom_toBottomOf="@+id/guideline656"
            />
        <TextView
            android:id="@+id/type"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:text="TextView"
            android:layout_marginTop="8dp"
            android:layout_marginRight="0dp"
            app:layout_constraintRight_toRightOf="@id/guideline671"
            android:layout_marginLeft="16dp"
            app:layout_constraintLeft_toLeftOf="@+id/guideline670"
            app:layout_constraintBottom_toBottomOf="@+id/guideline659"
            android:layout_marginBottom="0dp"
            app:layout_constraintTop_toTopOf="@+id/constraintLayout"
            />
        </android.support.constraint.ConstraintLayout>

    <ImageView
        android:id="@+id/boat"
        android:layout_width="0dp"
        android:layout_height="0dp"

        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/guideline61"
        app:layout_constraintTop_toTopOf="@+id/guideline64"
        android:layout_marginTop="100dp"
        android:layout_marginBottom="0dp"
        app:layout_constraintBottom_toTopOf="@+id/guideline62"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintVertical_bias="0.0" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline65"
        app:layout_constraintGuide_percent="1"

        android:orientation="vertical" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline66"
        app:layout_constraintGuide_percent="0.03"
        android:orientation="vertical" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline67"
        app:layout_constraintGuide_percent="0.41"
        android:orientation="vertical" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline68"
        app:layout_constraintGuide_percent="0.33"
        android:orientation="horizontal" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline70"
        app:layout_constraintGuide_percent="0.78"
        android:orientation="horizontal" />



    <Button
        android:id="@+id/catchbutton"
        android:layout_width="120dp"
        android:layout_height="0dp"

        android:background="@drawable/catchbutton"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/guideline61"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintTop_toTopOf="@+id/guideline70"
        android:layout_marginTop="8dp" />

</android.support.constraint.ConstraintLayout>